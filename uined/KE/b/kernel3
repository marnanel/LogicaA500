NEEDS"CAPCH"
NEEDS"COMPCH"
NEEDS"COMPSTR"
NEEDS"ENDPROG"
NEEDS"INOUT4"
NEEDS"ERRORMS"
NEEDS"FILETOV"
NEEDS"FINDARG"
NEEDS"GLOBIN"
NEEDS"LEVEL"
NEEDS"MAXVEC"
NEEDS"NEWLINE"
NEEDS"NEWPAGE"
NEEDS"RANDOM"
NEEDS"RDARGS"
NEEDS"RDITEM"
NEEDS"READN"
NEEDS"RUNPROG"
NEEDS"VECTOFI"
NEEDS"STACKSI"
NEEDS"TESTFLA"
NEEDS"VDU"
NEEDS"WRITED"
NEEDS"WRITEF2"
NEEDS"WRITEHE"
